import java.util.*;
class assignment1
{
public static void main(String args[])
{
ArrayList<Integer> a=new ArrayList<Integer>();
a.add(10);
a.add(2);
a.add(67);
ArrayList<Integer> b=new ArrayList<Integer>();
b.add(3);
b.add(30);
b.add(12);
ArrayList<Integer> c=new ArrayList<Integer>();
c.addAll(a);
c.addAll(b);
Collections.sort(c);
System.out.println(c);
}
}

